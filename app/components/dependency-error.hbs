
<AuCard as |c|>
    <c.header>
        Opgelet: deze producten zijn vereist voor {{@target.title}}

    </c.header>
    <c.content>
        {{#each @missingProducts as |product|}}
            <Product @product={{product}} @addToCart={{this.addToCart}} />
        {{/each}}
    </c.content>
</AuCard>